{
  "name": "conjoon",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "postversion": "git push && git push --tags",
    "release": "standard-version --commit-all",
    "release:patch": "standard-version --commit-all --release-as patch && npm run postversion",
    "release:minor": "standard-version --commit-all --release-as minor && npm run postversion",
    "release:major": "standard-version --commit-all --release-as major && npm run postversion"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-beta.20",
    "@docusaurus/plugin-client-redirects": "^2.0.0-beta.20",
    "@docusaurus/preset-classic": "^2.0.0-beta.20",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.1.1",
    "prism-react-renderer": "^1.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^8.14.0",
    "eslint-plugin-react": "^7.29.4",
    "standard-version": "^9.5.0"
  }
}
